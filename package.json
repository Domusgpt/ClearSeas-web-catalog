{
  "name": "clearseas-web-catalog",
  "version": "1.0.0",
  "description": "Clear Seas Web Catalog - Testing and Quality Assurance Suite",
  "scripts": {
    "test": "jest",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "test:visual:headed": "playwright test --headed",
    "test:all": "npm run lint && npm run test && npm run test:visual",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "serve": "http-server -p 8000 -c-1",
    "test:build": "node scripts/test-all-builds.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "testing",
    "visual-testing",
    "playwright",
    "jest",
    "web-catalog"
  ],
  "author": "Paul Phillips <Paul@clearseassolutions.com>",
  "license": "PROPRIETARY"
}
